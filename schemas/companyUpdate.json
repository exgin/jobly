{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "required": ["name"],
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Testing .Inc"]
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["test"]
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "format": "uri",
      "examples": ["google.com"]
    }
  }
}
